{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Authentication": {
    "Jwt": {
      "Authority": "https://login.microsoftonline.com/<TENANT_ID>/v2.0",
      "Audience": "<API_CLIENT_ID>"
    }
  },
  "Cors": {
    "AllowedOrigins": [
      "https://localhost:7127"
    ]
  },
  "Tenancy": {
    "DefaultTenantId": "dev-tenant-id"
  }
}