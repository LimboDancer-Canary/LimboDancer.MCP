{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Authentication": {
    "Jwt": {
      "Authority": "https://login.microsoftonline.com/{tenantId}/v2.0",
      "Audience": "{your-api-app-id-or-app-id-uri}"
    }
  },
  "Cors": {
    "AllowedOrigins": [ "https://localhost:5001", "https://localhost:7179" ]
  },
  "Tenancy": {
    "DefaultTenantId": "dev-tenant-id"
  }
}