        "Cors:AllowedOrigins": "http://localhost:5179;http://localhost:5000;https://localhost:5001"
      }
    },
    "LimboDancer.MCP.McpServer (HTTPS)": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": false,
      "applicationUrl": "https://localhost:7256",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "Server:BaseUrl": "https://localhost:7256",
        "Tenancy:HeaderName": "X-Tenant-Id",
        "Cors:AllowedOrigins": "https://localhost:7256;http://localhost:5000;https://localhost:5001"
      }
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": false,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "Server:BaseUrl": "http://localhost:5179",
        "Tenancy:HeaderName": "X-Tenant-Id"
      }
    }
  }
